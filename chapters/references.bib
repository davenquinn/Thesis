@article{Germs1974,
 abstract = {The Nama Group in South West Africa is subdivided slightly differently and in greater detail than before. The lower shelf deposits were supplied mainly from the easterly Kalahari shield and the more feldspathic and more detrital reddish upper sediments from uplifted regions of the inferred adjacent Damara geosyncline, which probably formed part of the extensive Pan-African Geosyncline to the west and north. There is much evidence of severe climatic conditions and volcanism having existed. This group and the underlying Gariep Group appear to represent a single geotectonic cycle, towards the end of which the Nama Group was deposited. The fauna found in the Nama Group seems to indicate a Vendian (Ediacarian) age for the lower Nama Group but the age for the upper Nama Group is uncertain. The Ediacara fauna in Australia and the related Nama fauna have only one identical fossil thus far. The Nama Group is a good test for the model of Valentine and Moores (1972) and in general confirms it. The Nama Group is considered to be younger than the base of the Mulden Group to the north.},
 author = {Germs, Gerard J. B.},
 doi = {10.1086/627966},
 issn = {0022-1376, 1537-5269},
 journal = {The Journal of Geology},
 language = {en},
 month = {May},
 number = {3},
 pages = {301-317},
 title = {The {{Nama Group}} in {{South West Africa}} and {{Its Relationship}} to the {{Pan}}-{{African Geosyncline}}},
 volume = {82},
 year = {1974}
}

