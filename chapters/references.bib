 isbn = {0958-1669},
 issn = {09581669},
 journal = {Current Opinion in Biotechnology},
 number = {2},
 pages = {153-159},
 pmid = {18374559},
 title = {What Is the Maximum Efficiency with Which Photosynthesis Can Convert Solar Energy into Biomass?},
