@article{Ducea2003b,
 abstract = {Apatite (U-Th)/He ages from a vertical transect through the Santa Lucia Mountains, central California Coast Ranges, are used to reconstruct the history of exhumation and of bedrock and surface uplift in this region since ca. 6 Ma. We find a direct correlation between (U-Th)/He ages and elevation, which we interpret to correspond to denudation rates of approximately 0.35 mm/yr between 6 and 2 Ma. The onset of bedrock uplift and exhumation ca. 6 Ma followed a change in plate motion ca. 8 Ma. After 2 Ma, denudation rates increased substantially ( approximately 0.9 mm/yr). This is a rare instance in which long-term average bedrock ( approximately 0.85 mm/yr) and surface ( approximately 0.20 mm/yr) uplift can be calculated from denudation rates and stratigraphic data. The post-2 Ma denudation rate is about one order of magnitude higher than independently determined river erosion rates in the area. We suggest that this discrepancy indicates that exhumation of the steep western slopes of this segment of the Coast Ranges has been dominated by mass wasting via landslides, rather than fluvial erosion, at least since ca. 2 Ma. We also show that the bedrock uplift is predominantly tectonic, not isostatic.},
 author = {Ducea, Mihai and House, Martha A. and Kidder, Steven},
 doi = {10.1130/0091-7613(2003)031<0139:lcdaur>2.0.co;2},
 issn = {00917613},
 journal = {Geology},
 keywords = {California,Denudation,Transpression,Uplift},
 number = {2},
 pages = {139},
 publisher = {{Geological Society of America}},
 title = {Late {{Cenozoic}} Denudation and Uplift Rates in the {{Santa Lucia Mountains}}, {{California}}},
 volume = {31},
 year = {2003}
}

