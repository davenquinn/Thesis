@article{Cole1995,
 abstract = {Episodes of middle Cenozoic near-trench volcanism in California ocurred$\backslash$nduring the transition from convergent to transform plate boundaries as$\backslash$nsegments of the East Pacific Rise intersected a subduction zone along$\backslash$nwestern North America. Geochemical features of volcanic rocks from the$\backslash$nCoast Range Province and Santa Maria Province, which represent two$\backslash$nnear-trench volcanic episodes, indicate that magmas from each province$\backslash$nwere derived from depleted mantle and evolved by assimilation-fractional$\backslash$ncrystallization processes to form predominantly bimodal suites. Basalt$\backslash$nand basaltic andesite from both provinces yielded epsilon(Nd)(t) values$\backslash$nbetween +9.3 and +2.4 and Sr-87/Sr-86(t) ratios of 0.702 58-0.706 72. $\backslash$nThe observed epsilon(Nd)(t) values that cluster around +9 and the$\backslash$nSr-87/Sr-86(t) ratios  0.7029 imply a source of depleted mantle,$\backslash$nanalogous to mid-ocean-ridge basalt (MORB) sources, for these rocks. $\backslash$nTh/Ta and Ba/Ta ratios as low as 0.49 and 35.78, respectively, for the$\backslash$nbasalt are similar to those of MORB and also suggest a magma source from$\backslash$ndepleted mantle. Acidic rocks, including rhyolite, dacite, and trachyte$\backslash$nsamples have epsilon(Nd)(t) values between +6.3 and -3.2 and$\backslash$nSr-87/Sr-86(t) ratios of 0.703 93 to 0.711 31. The variation among the$\backslash$nCoast Range and Santa Maria Provinces volcanic rocks in Nd-Sr isotope$\backslash$nratio space suggests that mixing occurred between the depleted$\backslash$nmantle-derived basaltic end-member and an incompatible-element-enriched$\backslash$ncrustal reservoir through which these rocks erupted. The observed$\backslash$nnegative correlation of epsilon(Nd)(t) and positive correlation of$\backslash$nSr-87/Sr-86(t) ratios with SiO2, respectively, also suggest assimilation$\backslash$nof an isotopically distinct crustal component by depleted mantle-derived$\backslash$nmelts. The ages and palegeographic distributions of these volcanic$\backslash$nrocks indicate that they were erupted during episodes when segments of$\backslash$nthe East Pacific Rise intersected southern California. Depleted mantle$\backslash$nthat was emplaced beneath the continental margin during ridge subduction$\backslash$nbecame a source of magma for the episodes of near-trench volcanism as a$\backslash$nnew strike-slip regime evolved along the continental margin.},
 author = {Cole, R. B. and Basu, A. R.},
 doi = {10.1130/0016-7606(1995)107<0167:NSIGAT>2.3.CO;2},
