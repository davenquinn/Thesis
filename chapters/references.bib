@article{Zuber2001,
 abstract = {Clues to the history of Mars are recorded in the chemistry and structure of the planet's crust and mantle. The mantle is the rocky, interior region of the planet that transports heat generated during accretion and subsequent core formation. The crust formed by melting of the upper mantle, and has been shaped and re-distributed by impact, volcanism, mantle flow and erosion. Observations point to a dynamically active interior in the early phases of martian history, followed by a rapid fall-off in heat transport that significantly influenced the geological, geophysical and geochemical evolution of the planet, including the history of water and climate.},
 author = {Zuber, M T},
 doi = {10.1038/35084163},
