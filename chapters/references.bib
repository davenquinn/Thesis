 issn = {10902643},
 journal = {Icarus},
 keywords = {Mars,climate,surface,Geological processes},
 pages = {329-351},
 publisher = {{Elsevier Inc.}},
 title = {Landscape Formation at the {{Deuteronilus}} Contact in Southern {{Isidis Planitia}}, {{Mars}}: {{Implications}} for an {{Isidis Sea}}?},
 volume = {242},
